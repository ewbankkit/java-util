-->
<!--
// Test environment.
-->
<Configuration status="info">
  <Properties/>

  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%date{yyyy/MM/dd HH:mm:ss.SSS} %-5level [%thread] [%logger{36}] %message%n"/>
    </Console>
  </Appenders>

  <Loggers>
    <Root level="info">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>
